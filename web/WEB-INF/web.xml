<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ContactUs</servlet-name>
        <servlet-class>controller.ContactUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteNGO</servlet-name>
        <servlet-class>controller.DeleteNGO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteNGOData</servlet-name>
        <servlet-class>controller.DeleteNGOData</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayNGO</servlet-name>
        <servlet-class>controller.DisplayNGO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddNewNGO</servlet-name>
        <servlet-class>controller.AddNewNGO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateNgo</servlet-name>
        <servlet-class>controller.UpdateNgo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateNgoData</servlet-name>
        <servlet-class>controller.UpdateNgoData</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ContactUs</servlet-name>
        <url-pattern>/ContactUs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayNGO</servlet-name>
        <url-pattern>/DisplayNGO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddNewNGO</servlet-name>
        <url-pattern>/AddNewNGO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteNGO</servlet-name>
        <url-pattern>/DeleteNGO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteNGOData</servlet-name>
        <url-pattern>/DeleteNGOData</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateNgo</servlet-name>
        <url-pattern>/UpdateNgo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateNgoData</servlet-name>
        <url-pattern>/UpdateNgoData</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
